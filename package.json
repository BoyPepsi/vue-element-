{
  "name": "supervisor_pc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "rimraf -rf ./node_modules/.cache/vue-loader &&  vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "@d2-projects/d2-crud": "^2.1.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.4",
    "default-passive-events": "^1.0.10",
    "element-ui": "^2.13.2",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-bus": "^1.2.1",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.2.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-dupe-keys": 2,
      "no-dupe-args": 2,
      "no-else-return": 2,
      "no-empty": 2,
      "no-extra-parens": 2,
      "no-extra-semi": 2,
      "no-implicit-coercion": 1,
      "no-irregular-whitespace": 2,
      "no-multi-spaces": 1,
      "no-multiple-empty-lines": [
        1,
        {
          "max": 2
        }
      ],
      "no-redeclare": 2,
      "no-spaced-func": 2,
      "no-trailing-spaces": 1,
      "no-undef": 1,
      "no-undef-init": 2,
      "no-undefined": 2,
      "no-unreachable": 2,
      "no-unused-expressions": 2,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-use-before-define": 2,
      "no-var": 0,
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "arrow-parens": 0,
      "arrow-spacing": 0,
      "block-scoped-var": 0,
      "comma-dangle": [
        2,
        "never"
      ],
      "comma-spacing": 0,
      "comma-style": [
        2,
        "last"
      ],
      "dot-notation": [
        0,
        {
          "allowKeywords": true
        }
      ],
      "eqeqeq": 2,
      "key-spacing": [
        0,
        {
          "beforeColon": false,
          "afterColon": true
        }
      ],
      "object-curly-spacing": [
        0,
        "never"
      ],
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "semi-spacing": [
        0,
        {
          "before": false,
          "after": false
        }
      ],
      "space-before-function-paren": [
        0,
        "always"
      ],
      "space-in-parens": [
        0,
        "never"
      ],
      "space-infix-ops": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
